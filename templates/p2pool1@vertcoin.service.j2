[Unit]
Description=p2pool1
After=vertcoind.service

[Service]
Type=simple
User=%i
ExecStart=/home/vertcoin/p2pool-vtc/run_p2pool.py --net vertcoin --give-author {{ vertcoin_author_fee }} -f {{ vertcoin_admin_fee }} -a {{ vertcoin_admin_address_1 }} --min-difficulty {{ vertcoin_min_difficulty_1 }}

[Install]
WantedBy=multi-user.target
